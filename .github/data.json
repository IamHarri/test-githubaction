{
    "release/*": {
        "variable": ".env.stage",
        "front_domain": "stage.compliancy.teskos.cloud",
        "mailer_dns": "${{ secrets.DEV_MAILER_DNS}}"
    },
    "master": {
        "variable": ".env",
        "front_domain": "app.compliancy.fr",
      "mailer_dns": "${{ secrets.PROD_MAILER_DNS}}"
    },
    "develop": {
        "variable": ".env.dev",
        "front_domain": "dev.compliancy.teskos.cloud",
        "mailer_dns": "${{ secrets.DEV_MAILER_DNS}}"
    },
    ".*": {
      "variable": ".env.dev",
      "front_domain": "dev.compliancy.teskos.cloud",
      "mailer_dns": "${{ secrets.DEV_MAILER_DNS}}"
    }
}
